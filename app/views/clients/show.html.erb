<h1><%= @client.name %></h1>
<br>
<h3>Address: <%= @client.address %></h3>
<br>
 <h3>Medical History: <%= @client.medical_history%></h3>
 <br>
 <br>
<% #if current_user == @client.massage_spa%>
    <h3>To Edit your client, click <a href="/clients/<%= @client.id%>/edit">Here</a></h3>
    <br>
    <form action="/clients/<%= @client.id%>" method="post">
        <input type="hidden" name="_method" value="DELETE">
        <input type="submit" value="Delete Client">
    </form>
<% #end %>